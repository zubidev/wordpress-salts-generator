{
    "name": "rbdwllr/wordpress-salts-generator",
    "description": "A key and salts generator for WordPress that works with both wp-config and .env files.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zubidev",
            "email": "zubidev@gmail.com"
        }
    ],
    "keywords": ["wordpress", "salts", "keys", "cli"],
    "require": {
        "php": ">=7.0.0",
        "paragonie/random-lib": "2.0.*"
    },
    "require-dev": {
    	"phpunit/phpunit": "6.5.*",
        "phpstan/phpstan": "0.9.*",
        "phpmd/phpmd": "2.6.*"
    },
    "autoload": {
        "psr-4": {
            "WPSalts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },"bin": [
        "wpsalts"
    ]
}
